{
  "compilerOptions": {
    "composite": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "moduleResolution": "bundler",
    "noEmitOnError": false,
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022"
  }
}
