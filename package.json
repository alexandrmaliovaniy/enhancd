    "react-file-router:build": "npm run build --workspace=@enhancd/react-file-router",
    "disclosure:test": "npm run test --workspace=@enhancd/disclosure",
    "disclosure:build": "npm run build --workspace=@enhancd/disclosure",
    "examples:react-file-router": "npm run dev:react-file-router --workspace=examples",
    "examples:disclosure": "npm run dev:disclosure --workspace=examples"
  },
  "author": "",
  "license": "ISC",
